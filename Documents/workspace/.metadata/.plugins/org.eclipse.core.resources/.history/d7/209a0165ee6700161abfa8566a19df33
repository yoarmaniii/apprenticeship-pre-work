public class Applica {
	public static void main(String[] args) {
		
		int myInt = 37;
		
		if ( myInt < 10) {	
		System.out.println(" yes that is correct !");	
		}
		else if (myInt < 20){
			System.out.println("No thats not correct !");
		}
		else {
			System.out.println(" None of the above");
		}
		}
	
}
		